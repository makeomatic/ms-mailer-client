{
  "name": "ms-mailer-client",
  "version": "0.3.0",
  "description": "client for ms-mailer",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators ./src; fi",
    "prepublish": "npm run compile",
    "pretest": "npm rebuild",
    "test": "./node_modules/.bin/mocha --require ./test/babelhook.js --bail --timeout 10000 -R spec"
  },
  "keywords": [
    "microservice",
    "amqp",
    "nodemailer",
    "mailer",
    "smtp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/ms-mailer-client.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-mailer-client/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-mailer-client#readme",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.6",
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.1.0",
    "eslint-plugin-react": "^3.10.0",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "ms-validation": "^0.5.1"
  }
}
