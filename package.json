  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "common-errors": "^1.0.5",
    "eslint": "^4.15.0",
    "eslint-config-makeomatic": "^2.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
